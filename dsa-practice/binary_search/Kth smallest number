class Solution {
    public int kthSmallest(int n, int m, int k) {
        int low = 1;
        int high = n * m;

        while (low < high) {
            int mid = low + (high - low) / 2;
            int count = 0;

            for (int i = 1; i <= n; i++) {
                count += Math.min(mid / i, m);
            }

            if (count >= k) {
                high = mid;
            } else {
                low = mid + 1;
            }
        }

        return low;
    }
}
